<header>
        <nav class="navbar fixed-top navbar-light shadow-sm" style="left: 70px; background-color: white; height: 50px; border-bottom: 1px solid lightgray;">
            <div class="container-fluid" style="padding-inline: 24px;">
                <div class="d-flex align-items-center" style="gap: 100px;">
                    <div class="box-logo" style="height: 34px; width: 100px; background-color: #CFD4ED;"></div>
                    <button class="btn navbar-toggler-icon"></button>
                </div>
                <div class="d-flex gap-2">
                    <form class="d-flex">
                        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn" style="color: #0D28A6; border: 1px solid #0D28A6;" type="submit">Search</button>
                    </form>
                    <div class="d-flex gap-2 align-items-center">
                        <div style="background-color: #CFD4ED; border-radius: 50%; padding: 2px;">
                            <img src="/public/assets/images/anonym-avatar.png" alt="BCR Avatar" height="30" width="30">
                        </div>
                        <p id="email_user" style="margin: 0; font-size: 14px;"></p>
                        <svg xmlns="http://www.w3.org/2000/svg" height="20" width="20" viewBox="0 0 512 512"><!--!Font Awesome Free 6.5.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.--><path d="M233.4 406.6c12.5 12.5 32.8 12.5 45.3 0l192-192c12.5-12.5 12.5-32.8 0-45.3s-32.8-12.5-45.3 0L256 338.7 86.6 169.4c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3l192 192z"/></svg>
                    </div>
                </div>
            </div>
        </nav>
        <div style="height: 100vh; width: 200px; padding-top: 20px; left: 70px; top: 50px; background-color: white;" class="fixed-top">
            <h3 style="font-weight: 700; font-size: 14px; color: gray; padding-left: 24px;"><%= menu_title %></h3>
            <div style="background-color: #CFD4ED; padding-block: 7px;">
                <% listMenu.forEach((menu) => { %>
                    <p style="font-weight: 700; font-size: 14px; margin: 0; padding-left: 24px;"><%= menu %></p>
                <% }) %>
                
            </div>
        </div>
    </header>